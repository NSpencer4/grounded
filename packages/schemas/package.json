{
  "name": "@grounded/schemas",
  "version": "0.0.1",
  "description": "Shared schemas for Grounded",
  "type": "module",
  "main": "./index.ts",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "proto:generate": "buf generate proto",
    "proto:lint": "buf lint proto",
    "proto:breaking": "buf breaking proto --against .git#branch=main"
  },
  "exports": {
    ".": "./index.ts",
    "./events": "./events/index.ts",
    "./events/*": "./events/*.ts",
    "./test-data": "./test-data/index.ts",
    "./test-data/events": "./test-data/events/index.ts",
    "./test-data/events/*": "./test-data/events/*.ts",
    "./proto": "./generated/ts/index.ts",
    "./proto/*": "./generated/ts/*.ts"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.3"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.47.2",
    "@bufbuild/protoc-gen-es": "^2.2.3"
  }
}
