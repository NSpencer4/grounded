{
  "name": "@grounded/organization-api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist --format=esm --external:pg-native",
    "build:zip": "npm run build && cd dist && zip -r ../function.zip .",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "test:endpoints": "tsx src/test-endpoints.ts"
  },
  "dependencies": {
    "@grounded/server-shared": "*"
  },
  "devDependencies": {
    "drizzle-kit": "^0.30.5",
    "esbuild": "^0.24.2",
    "tsx": "^4.19.0"
  }
}
