{
  "name": "@grounded/organization-api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist --format=esm --external:pg-native",
    "build:zip": "npm run build && cd dist && zip -r ../function.zip .",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@grounded/server-shared": "*",
    "drizzle-orm": "^0.45.1",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.10.0",
    "@types/pg": "^8.11.11",
    "drizzle-kit": "^0.30.5",
    "esbuild": "^0.24.2",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
