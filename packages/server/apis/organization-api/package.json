{
  "name": "@grounded/organization-api",
  "version": "0.0.1",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed-comprehensive": "tsx scripts/seed-data.ts",
    "db:clear": "tsx scripts/clear-data.ts",
    "db:reset": "npm run db:clear && npm run db:seed-comprehensive",
    "test:endpoints": "tsx src/test-endpoints.ts"
  },
  "dependencies": {
    "@grounded/server-shared": "*",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@types/jsonwebtoken": "^9.0.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@types/passport-jwt": "^4.0.1",
    "drizzle-kit": "^0.30.5",
    "tsx": "^4.19.0"
  }
}
