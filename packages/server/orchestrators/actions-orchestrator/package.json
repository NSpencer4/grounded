{
  "name": "@grounded/actions-orchestrator",
  "version": "0.0.1",
  "description": "Lambda orchestrator for processing conversation command events",
  "type": "module",
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "package": "yarn run build && cd dist && zip -r function.zip .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node --experimental-vm-modules ../../../../node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ../../../../node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules ../../../../node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "@grounded/schemas": "*",
    "@grounded/server-shared": "*"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^30.0.0"
  }
}
