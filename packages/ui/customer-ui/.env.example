# GraphQL Gateway API (server-side)
GRAPHQL_ENDPOINT=http://localhost:8787/graphql

# GraphQL Gateway API (client-side - requires VITE_ prefix)
VITE_GRAPHQL_ENDPOINT=http://localhost:8787/graphql

# Organization ID (default for local development)
DEFAULT_ORG_ID=acme-corp
VITE_DEFAULT_ORG_ID=acme-corp

# Supabase - Auth Only (NO DATABASE ACCESS)
# All data operations use GraphQL. Supabase is only for authentication.
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
